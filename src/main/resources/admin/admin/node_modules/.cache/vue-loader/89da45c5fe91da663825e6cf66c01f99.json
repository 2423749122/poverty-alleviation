onfirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        this.$http({\n          url: \"xianshangzixun/delete\",\n          method: \"post\",\n          data: ids\n        }).then(({ data }) => {\n          if (data && data.code === 0) {\n            this.$message({\n              message: \"操作成功\",\n              type: \"success\",\n              duration: 1500,\n              onClose: () => {\n                this.search();\n              }\n            });\n          } else {\n            this.$message.error(data.msg);\n          }\n        });\n      });\n    },\n\n\n  }\n\n};\n</script>\n<style lang=\"scss\" scoped>\n\t\r\n\t.center-form-pv {\r\n\t  .el-date-editor.el-input {\r\n\t    width: auto;\r\n\t  }\r\n\t}\r\n\t\r\n\t.el-input {\r\n\t  width: auto;\r\n\t}\n\t\r\n\t// form\r\n\t.center-form-pv .el-input /deep/ .el-input__inner {\r\n\t\t\t\tborder: 0;\r\n\t\t\t\tpadding: 0 12px;\r\n\t\t\t\tcolor: #000;\r\n\t\t\t\tfont-size: 14px;\r\n\t\t\t\tborder-color: #FFD563;\r\n\t\t\t\tborder-radius: 0;\r\n\t\t\t\tbox-shadow: 0 0 0px rgba(64, 158, 255, .5);\r\n\t\t\t\toutline: none;\r\n\t\t\t\tbackground: #CFD5F8;\r\n\t\t\t\twidth: 170px;\r\n\t\t\t\tborder-width: 0 4px;\r\n\t\t\t\tborder-style: solid;\r\n\t\t\t\theight: 40px;\r\n\t\t\t}\r\n\t\r\n\t.center-form-pv .el-select /deep/ .el-input__inner {\r\n\t\t\t\tborder: 0;\r\n\t\t\t\tpadding: 0 10px;\r\n\t\t\t\tcolor: #000;\r\n\t\t\t\tfont-size: 14px;\r\n\t\t\t\tborder-color: #FFD563;\r\n\t\t\t\tborder-radius: 0;\r\n\t\t\t\tbox-shadow: 0 0 0px rgba(64, 158, 255, .5);\r\n\t\t\t\toutline: none;\r\n\t\t\t\tbackground: #CFD5F8;\r\n\t\t\t\twidth: 170px;\r\n\t\t\t\tborder-width: 0 4px;\r\n\t\t\t\tborder-style: solid;\r\n\t\t\t\theight: 40px;\r\n\t\t\t}\r\n\t\r\n\t.center-form-pv .el-date-editor /deep/ .el-input__inner {\r\n\t\t\t\tborder: 0;\r\n\t\t\t\tpadding: 0 10px 0 30px;\r\n\t\t\t\tcolor: #000;\r\n\t\t\t\tfont-size: 14px;\r\n\t\t\t\tborder-color: #FFD563;\r\n