gin\":\"0 0 20px 0\"}' v-if=\"flag=='xuesheng'\"  label=\"性别\" prop=\"xingbie\">\r\n          <el-select v-model=\"ruleForm.xingbie\"  placeholder=\"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in xueshengxingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}'   v-if=\"flag=='xuesheng'\"  label=\"年龄\" prop=\"nianling\">\r\n          <el-input v-model=\"ruleForm.nianling\"               placeholder=\"年龄\" clearable></el-input>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}'   v-if=\"flag=='xuesheng'\"  label=\"联系方式\" prop=\"lianxifangshi\">\r\n          <el-input v-model=\"ruleForm.lianxifangshi\"               placeholder=\"联系方式\" clearable></el-input>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}'  v-if=\"flag=='xuesheng'\" label=\"注册时间\" prop=\"zhuceshijian\">\r\n            <el-date-picker\r\n                format=\"yyyy 年 MM 月 dd 日\"\r\n                value-format=\"yyyy-MM-dd\"\r\n                v-model=\"ruleForm.zhuceshijian\" \r\n                type=\"date\"\r\n                placeholder=\"注册时间\">\r\n            </el-date-picker> \r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}'   v-if=\"flag=='xinlidaoshi'\"  label=\"导师工号\" prop=\"daoshigonghao\">\r\n          <el-input v-model=\"ruleForm.daoshigonghao\" readonly              placeholder=\"导师工号\" clearable></el-input>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}'   v-if=\"flag=='xinlidaoshi'\"  label=\"导师姓名\" prop=\"daoshixingming\">\r\n          <el-input v-model=\"ruleForm.daoshixingming\"          