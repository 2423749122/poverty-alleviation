lick=\"lookDati(scope)\">查看答题情况</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<!-- <el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"huidaer\"\n\t\t\t\t\tlabel=\"回答二\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.huidaer}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"huidasan\"\n\t\t\t\t\tlabel=\"回答三\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.huidasan}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"huidasi\"\n\t\t\t\t\tlabel=\"回答四\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.huidasi}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"huidawu\"\n\t\t\t\t\tlabel=\"回答五\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.huidawu}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column> -->\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"cepingshijian\"\n\t\t\t\t\tlabel=\"测评时间\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.cepingshijian}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"xuehao\"\n\t\t\t\t\tlabel=\"学号\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.xuehao}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"nicheng\"\n\t\t\t\t\tlabel=\"昵称\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.nicheng}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column width=\"300\" label=\"操作\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<!-- <el-button :style='{\"border\":\"5px solid#CFD5F8\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 4px 0\",\"outline\":\"none\",\"color\":\"#000\",\"borderRadius\":\"30px\",\"background\":\"#fff\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('wodeceping','查看')\" type=\"success\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id,'info')\">详情</el-button> -->\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"5px solid#CFD5F8\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 4px 0\",\"outline\":\"none\",\"color\":\"#000\",\"borderRadius\":\"30px\",\"background\":\"#fff\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('wodeceping','测评报告')\" type=\"success\" size=\"mini\" @click=\"cepingbaogaoCrossAddOrUpdateHandler(scope.row,'cross','','[1]','此次测评已出报告')\">测评报告</el-button>\n\t\t\t\t\t\t\t<!-- <el-button :style='{\"border\":\"5px solid #CFD5F8\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 4px 0\",\"outline\":\"none\",\"color\":\"#000\",\"borderRadius\":\"30px\",\"background\":\"#fff\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('wodeceping','修改')\" type=\"primary\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button> -->\n\n\n\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"5px solid #CFD5F8\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 4px 0\",\"outline\":\"none\",\"color\":\"#000\",\"borderRadius\":\"30px\",\"background\":\"#fff\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('wodeceping','删除') \" type=\"danger\" size=\"mini\" @click=\"deleteHandler(scope.row.id)\">删除</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t</el-table>\n\t\t\t\t<el-pagination\r\n\t\t\t\t\t@size-change=\"sizeChangeHandle\"\n\t\t\t\t\t@current-change=\"currentChangeHandle\"\n\t\t\t\t\t:current-page=\"pageIndex\"\n\t\t\t\t\tbackground\r\n\t\t\t\t\t:page-sizes=\"[10, 20, 30, 50]\"\r\n\t\t\t\t\t:page-size=\"pageSize\"\r\n\t\t\t\t\t:layout=\"layouts.join()\"\r\n\t\t\t\t\t:total=\"totalPage\"\r\n\t\t\t\t\tprev-text=\"<\"\r\n\t\t\t\t\tnext-text=\">\"\r\n\t\t\t\t\t:hide-on-single-page=\"true\"\r\n\t\t\t\t\t:style='{\"width\":\"100%\",\"padding\":\"0\",\"margin\":\"20px 0 0\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"fontWeight\":\"500\"}'\n\t\t\t\t></el-pagination>\n\t\t\t<!-- </div> -->\n\t\t</template>\r\n\t\t\n\t\t<!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n\t\t<add-or-update v-if=\"addOrUpdateFlag\" :parent=\"this\" ref=\"addOrUpdate\"></add-or-update>\n\n\t\t<cepingbaogao-cross-add-or-update v-if=\"cepingbaogaoCrossAddOrUpdateFlag\" :parent=\"this\" ref=\"cepingbaogaoCrossaddOrUpdate\"></cepingbaogao-cross-add-or-update>\n\n\n<el-dialog\n  title=\"情况\"\n  :visible.sync=\"dialogVisible\"\n  width=\"30%\"\n  >\r\n  <div style=\"font-size: 18px;\">总分：{{zongfen}}分</div>\r\n  <br />\n     <div v-for=\"item in rowquest\">\r\n\t\t <div>题目：{{item.title}}</div>\r\n\t\t <div>答案：{{item.answer}}(得分：<span style=\"color: red;\" v-text=\"item.score==undefined?0:item.score\"></span>分)</div>\r\n\t </div>\n</el-dialog>\n\n\t</div>\n</template>\r\n\n<script>\nimport axios from 'axios'\nimport AddOrUpdate from \"./add-or-update\";\nimport cepingbaogaoCrossAddOrUpdate from \"../cepingbaogao/add-or-update\";\nexport default {\n  data() {\n    return {\r\n      searchForm: {\n        key: \"\"\n      },\n      form:{},\n      dataList: [],\n      pageIndex: 1,\n      pageSize: 10,\n      totalPage: 0,\n      dataListLoading: false,\n      dataListSelections: [],\n      showFlag: true,\n      sfshVisiable: false,\n      shForm: {},\n      chartVisiable: false,\n      chartVisiable1: false,\n      chartVisiable2: false,\n      chartVisiable3: false,\n      chartVisiable4: false,\n      chartVisiable5: false,\n      addOrUpdateFlag:false,\n      cepingbaogaoCrossAddOrUpdateFla