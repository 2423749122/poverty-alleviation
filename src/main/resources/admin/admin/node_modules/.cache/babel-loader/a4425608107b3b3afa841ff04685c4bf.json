y=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}' class=\"select\" v-if=\"type!='info'\"  label=\"性别\" prop=\"xingbie\">\r\n\t\t\t\t\t<el-select :disabled=\"ro.xingbie\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in xingbieOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}' v-else class=\"input\" label=\"性别\" prop=\"xingbie\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingbie\"\r\n\t\t\t\t\t\tplaceholder=\"性别\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}' class=\"input\" v-if=\"type!='info'\"  label=\"从业时间\" prop=\"congyeshijian\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.congyeshijian\" placeholder=\"从业时间\" clearable  :readonly=\"ro.congyeshijian\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}' v-else class=\"input\" label=\"从业时间\" prop=\"congyeshijian\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.congyeshijian\" placeholder=\"从业时间\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item :style='{\"width\":\"33.3%\",\"margin\":\"0 0 20px 0\"}' class=\"input\" v-if=\"type!='info'\"  label=\"联系电话\" prop=\"lianxidianhua\">\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxidianhua\" placeholder=\"联系电话\" clearable  :readonly=\"ro.lianxidianhua\"></el-input>\r\n\t\